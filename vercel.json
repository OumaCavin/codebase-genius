{
  "name": "codebase-genius-api",
  "version": 2,
  "builds": [
    {
      "src": "api-frontend/api/main_api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api-frontend/api/main_api.py"
    }
  ],
  "functions": {
    "api-frontend/api/main_api.py": {
      "maxDuration": 60
    }
  }
}
